    <validationRules>
        <fullName>Cancelled_Reason</fullName>
        <active>true</active>
        <description>When status is changed to Cancelled then a cancelled reason is required</description>
        <errorConditionFormula>AND(ISPICKVAL( Status , &apos;Cancelled&apos;), ISBLANK( Cancelled_Reason__c )  )</errorConditionFormula>
        <errorDisplayField>Cancelled_Reason__c</errorDisplayField>
        <errorMessage>A Cancelled Reason is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Change_Status_from_Closed</fullName>
        <active>true</active>
        <description>Cannot change a Work Order Status from Closed

AND(PRIORVALUE( Status )&lt;&gt;&apos;Closed&apos;, 

ISCHANGED(Status), 

NOT( ISPICKVAL(Status, &apos;Closed&apos;) ))</description>
        <errorConditionFormula>ISCHANGED(Status) &amp;&amp;  TEXT( PRIORVALUE( Status) ) = &apos;Closed&apos;</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Unable to change a Work Order status from Closed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invoice_Created_WorkOrder</fullName>
        <active>true</active>
        <errorConditionFormula>Invoice_Created__c = true
&amp;&amp; NOT((PRIORVALUE(  Invoice_Created__c ))=false)</errorConditionFormula>
        <errorMessage>Cannot update record once invoice has been created</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invoice_Created_on_Close_Only</fullName>
        <active>true</active>
        <description>Can only check invoice created if Work Order is closed</description>
        <errorConditionFormula>NOT(ISPICKVAL( Status , &apos;Closed&apos;))&amp;&amp;  Invoice_Created__c   =True</errorConditionFormula>
        <errorDisplayField>Invoice_Created__c</errorDisplayField>
        <errorMessage>Invoice Created can only be checked if Work Order is closed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Not_New</fullName>
        <active>true</active>
        <description>Cannot change a status back to New</description>
        <errorConditionFormula>ISCHANGED(Status)  &amp;&amp; TEXT(Status)=&apos;New&apos;</errorConditionFormula>
        <errorDisplayField>Status</errorDisplayField>
        <errorMessage>Cannot change a Work order status back to new</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SLA_Required</fullName>
        <active>true</active>
        <description>If Service Contract has a SLA and no SLA is selected then an error will occur</description>
        <errorConditionFormula>AND ( SLA_Count__c &lt;&gt;0, ISBLANK( SLA__c ))</errorConditionFormula>
        <errorDisplayField>SLA__c</errorDisplayField>
        <errorMessage>This Service Contract has related SLA&apos;s, please select a SLA</errorMessage>
    </validationRules>
