    <fields>
        <fullName>SLA_Fix_Progress__c</fullName>
        <externalId>false</externalId>
        <formula>IF(SLA_Response_Minutes__c&gt;=0,
IF(1-ROUND(SLA_Fix_Hours__c/24,2)&gt;0.7,
  IMAGE(&quot;/resource/slds_colors/red.png&quot;,&quot;Over 70% SLA Reached&quot;,10,(( 1-ROUND(SLA_Fix_Hours__c/24,2))*200)),
  IF(1-ROUND(SLA_Fix_Hours__c/24,2)&gt;0.5,
    IMAGE(&quot;/resource/slds_colors/orange.png&quot;,&quot;Over 50% SLA Reached&quot;,10,(( 1-ROUND(SLA_Fix_Hours__c/24,2))*200)),
    IF(1-ROUND(SLA_Fix_Hours__c/24,2)&gt;0.3,
      IMAGE(&quot;/resource/slds_colors/green.png&quot;,&quot;Over 30% SLA Reached&quot;,10,(( 1-ROUND(SLA_Fix_Hours__c/24,2))*200)),
      IMAGE(&quot;/resource/slds_colors/neutral-1.png&quot;,&quot;Under 30% SLA Reached&quot;,10,(( 1-ROUND(SLA_Fix_Hours__c/24,2))*200))
    )
  )
) &amp; &quot; &quot; &amp; TEXT(ROUND(SLA_Fix_Hours__c,2)) &amp; &quot; hrs remain&quot;
,
IMAGE(&quot;/resource/slds_colors/red.png&quot;,&quot;100% SLA Reached&quot;,10,((1)*200)) &amp; &quot; 0 hrs remain&quot;
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>SLA Fix Progress</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
