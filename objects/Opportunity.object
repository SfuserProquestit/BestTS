    <fields>
        <fullName>New_Opportunity2__c</fullName>
        <description>Determines if an opportunity was created last week in the form that is required for &quot;Sales Report - Open Opportunties&quot;. Formula taken from this forum: https://success.salesforce.com/ideaview?id=08730000000KFAUAA4</description>
        <externalId>false</externalId>
        <formula>case(mod(today() - DATE(1985,6,24),7),
6, if(DATEVALUE(CreatedDate) - today() &lt;=0, if(DATEVALUE(CreatedDate) - today() &gt;-7,&quot;This Week&quot;,if(today() -   DATEVALUE(CreatedDate) &lt;=14,&quot;Last Week&quot;,&quot;&quot;)),if(DATEVALUE(CreatedDate) - today() &lt;8,&quot;Next Week&quot;,&quot;&quot;)), 
0, if(DATEVALUE(CreatedDate) - today() &lt;=7, if(DATEVALUE(CreatedDate) - today() &gt;-1,&quot;This Week&quot;,if(today() -   DATEVALUE(CreatedDate) &lt;=8,&quot;Last Week&quot;,&quot;&quot;)),if(DATEVALUE(CreatedDate) - today() &lt;15,&quot;Next Week&quot;,&quot;&quot;)), 
1, if(DATEVALUE(CreatedDate) - today() &lt;=6, if(DATEVALUE(CreatedDate) - today() &gt;-2,&quot;This Week&quot;,if(today() -   DATEVALUE(CreatedDate) &lt;=9,&quot;Last Week&quot;,&quot;&quot;)),if(DATEVALUE(CreatedDate) - today() &lt;14,&quot;Next Week&quot;,&quot;&quot;)), 
2, if(DATEVALUE(CreatedDate) - today() &lt;=5, if(DATEVALUE(CreatedDate) - today() &gt;-3,&quot;This Week&quot;,if(today() -   DATEVALUE(CreatedDate) &lt;=10,&quot;Last Week&quot;,&quot;&quot;)),if(DATEVALUE(CreatedDate) - today() &lt;13,&quot;Next Week&quot;,&quot;&quot;)), 
3, if(DATEVALUE(CreatedDate) - today() &lt;=4, if(DATEVALUE(CreatedDate) - today() &gt;-4,&quot;This Week&quot;,if(today() -   DATEVALUE(CreatedDate) &lt;=11,&quot;Last Week&quot;,&quot;&quot;)),if(DATEVALUE(CreatedDate) - today() &lt;12,&quot;Next Week&quot;,&quot;&quot;)), 
4, if(DATEVALUE(CreatedDate) - today() &lt;=3, if(DATEVALUE(CreatedDate) - today() &gt;-5,&quot;This Week&quot;,if(today() -   DATEVALUE(CreatedDate) &lt;=12,&quot;Last Week&quot;,&quot;&quot;)),if(DATEVALUE(CreatedDate) - today() &lt;11,&quot;Next Week&quot;,&quot;&quot;)), 
5, if(DATEVALUE(CreatedDate) - today() &lt;=2, if(DATEVALUE(CreatedDate) - today() &gt;-6,&quot;This Week&quot;,if(today() -   DATEVALUE(CreatedDate) &lt;=13,&quot;Last Week&quot;,&quot;&quot;)),if(DATEVALUE(CreatedDate) - today() &lt;10,&quot;Next Week&quot;,&quot;&quot;)),
 &quot;&quot;)</formula>
        <label>New Opportunity</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
