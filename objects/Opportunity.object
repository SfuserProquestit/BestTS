    <sharingModel>Read</sharingModel>
    <validationRules>
        <fullName>Amount_Gross_Margin_Value_Required</fullName>
        <active>true</active>
        <description>When Closed Won, Gross Margin Value required</description>
        <errorConditionFormula>AND(
ISPICKVAL( StageName, &quot;Closed Won&quot;), 
ISNULL(Amount)
)</errorConditionFormula>
        <errorDisplayField>Amount</errorDisplayField>
        <errorMessage>Gross Margin Value (Amount) can not be empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Reason</fullName>
        <active>true</active>
        <description>If Closed select Reason__c</description>
        <errorConditionFormula>AND(IsClosed, ISPICKVAL(Reason__c, &quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Reason__c</errorDisplayField>
        <errorMessage>Please select Reason for Closing Opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Won_Engagement_Type_Start_Date</fullName>
        <active>true</active>
        <description>If Opportunity is marked as Closed/Won, Engagement Type must be selected and a Start Date must be set.</description>
        <errorConditionFormula>AND(ISPICKVAL( StageName, &quot;Closed Won&quot;), OR(ISBLANK(Start_Date__c),ISBLANK(End_Date__c),ISPICKVAL(Engagement_Type__c, &quot;&quot;)
)
)</errorConditionFormula>
        <errorMessage>Please select Engagement Type and set Start and End Date  for Close/Won Opportunities</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Won_Sales_Details_Required</fullName>
        <active>true</active>
        <description>Required Sales Details when an Opportunity is Close Won</description>
        <errorConditionFormula>AND(
  ISPICKVAL( StageName, &quot;Closed Won&quot;), 
  OR(
    ISPICKVAL( Strategic_Or_Tactical__c , &quot;&quot;),   
    Engineering_Revenue__c&lt;=0,
    ISNULL(Engineering_Revenue__c),
    ISBLANK( Engineering_Margin__c),
    ISBLANK( Project_Lead__c)
  )
    
)</errorConditionFormula>
        <errorMessage>All Closed Won Opportunities will require the following fields to be completed:
Strategic or Tactical Pick list, Engineering Revenue &amp; Margin and the Project Lead Contact</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Gross_Margin_Inserted_on_Closed_Stages</fullName>
        <active>true</active>
        <description>All Closed Won or Lost opportunities must include Gross Margin Value before closing stage</description>
        <errorConditionFormula>AND(
IsClosed,  
OR( Amount &lt;= 0, ISNULL(Amount))
)</errorConditionFormula>
        <errorDisplayField>Amount</errorDisplayField>
        <errorMessage>Gross Margin Value (Amount) is required if opportunity is Closed Won or Lost</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Reason_Required</fullName>
        <active>true</active>
        <description>Must specify other reason if no specific Reason is picked</description>
        <errorConditionFormula>AND(
  ISPICKVAL(Reason__c , &quot;Other&quot;),
   ISBLANK( Other_Reason__c)
)</errorConditionFormula>
        <errorDisplayField>Other_Reason__c</errorDisplayField>
        <errorMessage>Please specific other reason for Closing this Opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Total_Revenue_Inserted_on_Closed_Stages</fullName>
        <active>false</active>
        <description>All Closed Won or Lost opportunities must include Total Revenue before closing stage</description>
        <errorConditionFormula>AND(
IsClosed,  
OR(Total_Revenue__c &lt;= 0, ISNULL(Total_Revenue__c))
