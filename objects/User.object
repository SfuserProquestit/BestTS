    <validationRules>
        <fullName>Select_Engagement_Type_Notifications</fullName>
        <active>true</active>
        <description>Users that have &quot;Notify Closed Won Opportunities &quot; checked require the multi-picklist &quot;Engagement Type Notifications&quot; field to be completed</description>
        <errorConditionFormula>AND(
ISBLANK( Engagement_Type_Notifications__c), Notify_Closed_Won_Opportunities__c =true
)</errorConditionFormula>
        <errorDisplayField>Engagement_Type_Notifications__c</errorDisplayField>
        <errorMessage>Please select at lease one Engagement Type to send notifications for Closed/Won opportunities</errorMessage>
    </validationRules>
